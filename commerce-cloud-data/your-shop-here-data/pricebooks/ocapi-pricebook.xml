<?xml version="1.0" encoding="UTF-8"?>
<pricebooks xmlns="http://www.demandware.com/xml/impex/pricebook/2006-10-31">
    <pricebook>
        <header pricebook-id="ocapi-pricebook">
            <currency>EUR</currency>
            <display-name xml:lang="x-default">OCAPI Prices</display-name>
            <online-flag>true</online-flag>
        </header>

        <price-tables>
            <price-table product-id="ocapi-simple-product">
                <amount quantity="1">100.00</amount>
            </price-table>
            <price-table product-id="ocapi-simple-csg-product">
                <amount quantity="1">100.00</amount>
            </price-table>
            <price-table product-id="ocapi-simple-csg-product2">
                <amount quantity="1">100.00</amount>
            </price-table>
        </price-tables>
    </pricebook>

    <pricebook>
        <header pricebook-id="ocapi-sales-pricebook">
            <currency>EUR</currency>
            <display-name xml:lang="x-default">OCAPI Salees Prices</display-name>
            <online-flag>true</online-flag>
            <parent>ocapi-pricebook</parent>
        </header>

        <price-tables>
            <price-table product-id="ocapi-simple-product">
                <amount quantity="1">99.00</amount>
            </price-table>
            <price-table product-id="ocapi-simple-csg-product">
                <amount quantity="1">99.00</amount>
            </price-table>
            <price-table product-id="ocapi-simple-csg-product2">
                <amount quantity="1">99.00</amount>
            </price-table>
        </price-tables>
    </pricebook>
</pricebooks>